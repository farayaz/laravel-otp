{
    "name": "farayaz/laravel-otp",
    "description": "simple otp for laravel",
    "authors": [
        {
            "name": "Mehrdad",
            "email": "mehrdad.motaghifar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/container": "^10.0|^11.0"
    },
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Farayaz\\LaravelOtp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Farayaz\\LaravelOtp\\LaravelOtpServiceProvider"
            ],
            "aliases": {
                "OTP": "Farayaz\\LaravelOtp\\LaravelOtpFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.0"
    }
}
